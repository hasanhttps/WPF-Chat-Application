             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPF_Chat_Application_Homework"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="Ellipsis" UriSource="assets/icons8-ellipsis-v-24.png"/>
            <BitmapImage x:Key="Search" UriSource="assets/icons8-search-24.png"/>
            <BitmapImage x:Key="Edit" UriSource="assets/icons8-edit-24.png"/>
            <BitmapImage x:Key="SendMessage" UriSource="assets/icons8-send-message-48.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
